{
	"name": "CopyPipeline_FromDailyFires",
	"properties": {
		"description": "Extracts API data from https://data.sfgov.org/",
		"activities": [
			{
				"name": "Copy_jhk",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Destination",
						"value": "dailyFires/@{formatDateTime(pipeline().parameters.windowStart,'yyyy')}@{formatDateTime(pipeline().parameters.windowStart,'MM')}@{formatDateTime(pipeline().parameters.windowStart,'dd')}/@{formatDateTime(pipeline().parameters.windowStart,'yyyy')}@{formatDateTime(pipeline().parameters.windowStart,'MM')}@{formatDateTime(pipeline().parameters.windowStart,'dd')}_Fires_Raw_Data"
					}
				],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "arrayOfObjects"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "Source_FromAPI",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Destination_StorageAccount",
						"type": "DatasetReference",
						"parameters": {
							"cw_fileName": {
								"value": "@concat(formatDateTime(pipeline().parameters.windowStart,'yyyy'),formatDateTime(pipeline().parameters.windowStart,'MM'),formatDateTime(pipeline().parameters.windowStart,'dd'),'_Fires_Raw_Data')",
								"type": "Expression"
							},
							"cw_folderPath": {
								"value": "@concat(formatDateTime(pipeline().parameters.windowStart,'yyyy'),formatDateTime(pipeline().parameters.windowStart,'MM'))",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"windowStart": {
				"type": "String"
			}
		},
		"annotations": [],
		"lastPublishTime": "2021-10-27T14:27:37Z"
	},
	"type": "Microsoft.Synapse/workspaces/pipelines"
}